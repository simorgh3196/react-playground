{
  "name": "react-playground",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^20.0.8",
    "@types/node": "^8.0.26",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-redux": "^5.0.6",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-scripts-ts": "2.6.0",
    "redux": "^3.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/src/**/*.test.ts?(x)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.test.ts?(x)"
    ]
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "jest": "jest",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.6",
    "enzyme": "^2.9.1",
    "react-addons-test-utils": "^15.6.0",
    "ts-jest": "^20.0.14"
  }
}
